	<parameter name="PARAM_FILIALE_ID" class="java.lang.Integer"/>
	<parameter name="PARAM_ABHOLDATUM_VON" class="java.util.Date"/>
	<parameter name="PARAM_ABHOLDATUM_BIS" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
     REPARATUR."KUNDE_ID" AS REPARATUR_KUNDE_ID,
     REPARATUR."NUMMER" AS REPARATUR_NUMMER,
     REPARATUR."GERAET" AS REPARATUR_GERAET,
     REPARATUR."SERIENNUMMER" AS REPARATUR_SERIENNUMMER,
     REPARATUR."SYMPTOME" AS REPARATUR_SYMPTOME,
     REPARATUR."EXPRESSBEABEITUNG" AS REPARATUR_EXPRESSBEABEITUNG,
     REPARATUR."ABHOLDATUM" AS REPARATUR_ABHOLDATUM,
     REPARATUR."ABHOLZEIT" AS REPARATUR_ABHOLZEIT,
     REPARATUR."MITARBEITER_ID" AS REPARATUR_MITARBEITER_ID,
     REPARATUR."ERLEDIGUNGSDATUM" AS REPARATUR_ERLEDIGUNGSDATUM,
     MITARBEITER."ID" AS MITARBEITER_ID,
     MITARBEITER."NACHNAME" AS MITARBEITER_NACHNAME,
     MITARBEITER."VORNAME" AS MITARBEITER_VORNAME,
     FILIALE."NAME" AS FILIALE_NAME,
     FILIALE."STRASSE" AS FILIALE_STRASSE,
     FILIALE."PLZ" AS FILIALE_PLZ,
     FILIALE."ORT" AS FILIALE_ORT,
     FILIALE."EMAIL" AS FILIALE_EMAIL,
     FILIALE."TELEFON" AS FILIALE_TELEFON,
     KUNDE."NACHNAME" AS KUNDE_NACHNAME,
     KUNDE."VORNAME" AS KUNDE_VORNAME
FROM "PUBLIC"."REPARATUR" REPARATUR
INNER JOIN "PUBLIC"."MITARBEITER" MITARBEITER ON MITARBEITER.ID = REPARATUR.MITARBEITER_ID
INNER JOIN "PUBLIC"."FILIALE" FILIALE ON FILIALE.ID = $P{PARAM_FILIALE_ID}
INNER JOIN "PUBLIC"."KUNDE" KUNDE ON KUNDE.ID = REPARATUR.KUNDE_ID
WHERE REPARATUR.ABHOLDATUM >= $P{PARAM_ABHOLDATUM_VON} AND REPARATUR.ABHOLDATUM <= $P{PARAM_ABHOLDATUM_BIS}
ORDER BY REPARATUR.ABHOLDATUM, REPARATUR.ABHOLZEIT]]>
	</queryString>
